{
	"name": "pdf-parse",
	"version": "1.0.0",
	"type": "module",
	"main": "index.js",
	"description": "Pure JavaScript cross-platform module to extract texts from PDFs.",
	"homepage": "https://github.com/mehmet-kozan/pdf-parse#readme",
	"bugs": {
		"url": "https://github.com/mehmet-kozan/pdf-parse/issues"
	},
	"keywords": [
		"pdf parse",
		"pdf.js",
		"pdf-parse"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mehmet-kozan/pdf-parse.git"
	},
	"license": "ISC",
	"author": "",
	"scripts": {
		"build": "npm-run-all clean build-esm build-cjs",
		"build-esm": "tsc --project tsconfig.build.json --outDir dist/esm",
		"build-cjs": "tsc --project tsconfig.build.json --outDir dist/cjs --module commonjs --moduleResolution node --verbatimModuleSyntax false",
		"clean": "rimraf --glob dist test/**/*.txt",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"coverage": "vitest run --coverage",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"format:all": "biome check --write .",
		"format:check": "biome check ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.3",
		"npm-run-all": "^4.1.5",
		"pdfjs-dist": "^5.4.149",
		"rimraf": "^6.0.1",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=18"
	}
}
