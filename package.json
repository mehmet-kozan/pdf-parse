{
	"name": "pdf-parse",
	"version": "2.1.4",
	"type": "module",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"description": "Pure JavaScript cross-platform module to extract texts from PDFs.",
	"homepage": "https://github.com/mehmet-kozan/pdf-parse#readme",
	"bugs": {
		"url": "https://github.com/mehmet-kozan/pdf-parse/issues"
	},
	"keywords": [
		"pdf",
		"pdf-parse",
		"pdf-parser",
		"pdfjs",
		"pdf-text-extraction",
		"pdf-image-extraction",
		"pdf-table-extractor",
		"pdf-table-extraction",
		"table-extraction",
		"table-extractor",
		"table-parser",
		"pdf-to-text",
		"text-extraction",
		"pdf-ocr",
		"document-parser",
		"pdf-tools",
		"pdf-reader",
		"pdf-utils",
		"pdf-screenshot",
		"pdf-to-image",
		"server-side-pdf-parsing"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mehmet-kozan/pdf-parse.git"
	},
	"license": "Apache-2.0",
	"author": "Mehmet Kozan",
	"files": [
		"dist/",
		"dist/**/*.d.ts",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "npm-run-all clean build:esm build:cjs build:browser build:browser:min",
		"build:esm": "tsc --project tsconfig.esm.json",
		"build:cjs": "tsc --project tsconfig.cjs.json",
		"build:browser": "cross-env MINIFY=false vite build --minify=false",
		"build:browser:min": "cross-env MINIFY=true vite build --minify=terser",
		"clean": "rimraf --glob dist test/**/*.txt test/**/imgs",
		"prepare": "npm run build",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"coverage": "vitest run --coverage",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"format:all": "biome check --write .",
		"format:check": "biome check .",
		"pkg:check": "npm pack --dry-run",
		"serve:example": "node ./example/serve-example.js"
	},
	"dependencies": {
		"pdfjs-dist": "^5.4.149"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.3",
		"@types/node": "^24.3.1",
		"cross-env": "^10.1.0",
		"express": "^5.1.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^6.0.1",
		"terser": "^5.44.0",
		"typescript": "^5.9.2",
		"vite": "^7.1.5",
		"vitest": "^3.2.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=16"
	},
	"sideEffects": false
}
