{
	"name": "pdf-parse",
	"version": "2.3.12",
	"type": "module",
	"main": "dist/cjs/index.cjs",
	"module": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",

	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"browser": "./dist/browser/pdf-parse.es.js",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs"
		},
		"./node": {
			"types": "./dist/esm/index.d.ts",
			"require": "./dist/node/index.cjs"
		},
		"./worker": {
			"import": {
				"types": "./bin/worker/worker.d.ts",
				"default": "./bin/worker/worker.js"
			},
			"require": {
				"types": "./bin/worker/worker.d.cts",
				"default": "./bin/worker/worker.cjs"
			}
		},
		"./webworker": {
			"import": {
				"types": "./bin/worker/webworker.d.ts",
				"default": "./bin/worker/webworker.js"
			}
		}
	},

	"description": "Pure TypeScript, cross-platform module for extracting text, images, and tabular data from PDFs. Run directly in your browser or in Node!",
	"homepage": "https://mehmet-kozan.github.io/pdf-parse/",
	"bugs": {
		"url": "https://github.com/mehmet-kozan/pdf-parse/issues"
	},

	"keywords": [
		"pdf",
		"pdf-parse",
		"pdf-parser",
		"pdf-tools",
		"pdf-utils",
		"pdf.js",
		"pdfjs-dist",
		"pdf-text-extraction",
		"pdf-image-extraction",
		"pdf-table-extractor",
		"pdf-table-extraction",
		"pdf-screenshot",
		"pdf-thumbnail",
		"pdf-table-parser",
		"pdf-to-text",
		"pdf2text",
		"pdf-to-json",
		"pdf2json",
		"pdf-to-image",
		"pdf2image",
		"pdf-to-csv",
		"pdf2csv",
		"pdf-document-parser",
		"pdf-reader",
		"server-side-pdf-parsing"
	],

	"repository": {
		"type": "git",
		"url": "git+https://github.com/mehmet-kozan/pdf-parse.git"
	},

	"license": "Apache-2.0",
	"author": "Mehmet Kozan",
	"files": [
		"dist/",
		"src/",
		"bin/"
	],
	"scripts": {
		"build": "npm run clean && npm run build:ts && npm run build:node && npm run build:browser && npm run build:worker",
		"build:ts": "tsc && tsc --project tsconfig.node.json && node scripts/rename-cjs.mjs",
		"build:node": "vite build --config vite.config.node.ts",
		"build:browser": "vite build --config vite.config.browser.ts && vite build --config vite.config.browser.min.ts",
		"build:worker": "vite build --config vite.config.worker.ts",

		"clean": "npm run clean:build && npm run clean:site && npm run clean:test && npm run clean:test:i",
		"clean:build": "rimraf dist bin/worker/worker_source.js bin/worker/worker_source.cjs",
		"clean:site": "rimraf reports_site/test-report reports_site/coverage reports_site/live_demo/dist-browser",
		"clean:test": "rimraf --glob test/**/*.txt test/**/imgs test/**/*_images",
		"clean:test:i": "rimraf --glob test_integration/**/node_modules test_integration/**/package-lock.json test_integration/**/dist",

		"test": "vitest run --reporter=default",
		"test:i": "node scripts/integration.test.mjs",
		"test:ui": "vitest --ui --coverage",
		"test:watch": "vitest",

		"coverage": "vitest run --coverage",

		"bench": "npm run bench:install && vitest bench --run",
		"bench:install": "npm install --no-save pdf2json",

		"site": "npm run site:build && vite preview --outDir reports_site",
		"site:build": "npm run build && npm run coverage && npm run bench",

		"lint": "biome lint .",
		"format": "biome format --write .",
		"format:all": "biome check --write .",
		"format:check": "biome check .",

		"prepare": "npm run build",
		"pack": "npm pack --dry-run"
	},

	"dependencies": {
		"pdfjs-dist": "^5.4.296"
	},

	"optionalDependencies": {
		"@napi-rs/canvas": "^0.1.80"
	},

	"devDependencies": {
		"vite": "^7.1.5",
		"vitest": "^3.2.4",
		"typescript": "^5.9.3",
		"@biomejs/biome": "^2.2.6",
		"@types/node": "^24.7.2",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"rimraf": "^6.0.1"
	},

	"publishConfig": {
		"access": "public",
		"provenance": true
	},

	"engines": {
		"node": ">=20.16.0 <21 || >=22.3.0"
	},

	"browser": {
		"canvas": false,
		"fs": false,
		"http": false,
		"https": false,
		"url": false
	},

	"jsdelivr": "./dist/browser/pdf-parse.es.min.js",
	"unpkg": "./dist/browser/pdf-parse.es.min.js"
}
